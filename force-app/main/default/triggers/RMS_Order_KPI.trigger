/**
 * (包括WSL和DTC)记录Order自己的KPI日期
 */
trigger RMS_Order_KPI on RTV_Order__c (before update) {
    
    for (RTV_Order__c newOrd: Trigger.new) {
        RTV_Order__c oldOrd = Trigger.isUpdate? trigger.oldMap.get(newOrd.Id): null;
            
        // 实际日期: POST LF
        if (oldOrd.Status__c == 'Ready' && newOrd.Status__c == 'POST to LF') {
            newOrd.Actual_Date_Of_PTLF__c = System.today();
        }
        
        // 实际日期: Delivered(提货日)              *参考RMS_Order_Date_Compute.trigger
        // 实际日期: Delivered_Completed(到货日)    *参考RMS_Order_Date_Compute.trigger
        
        // 实际日期: 提交 Inspect 
        else if (oldOrd.Status__c == 'Inspected' && newOrd.Status__c == 'Insp Wait Approval') {
            newOrd.Actual_Date_Of_Inspected__c = System.today();
        }
        // 实际日期: CS审批 
        else if (oldOrd.Insp_CS_Approve_Time__c == null && newOrd.Insp_CS_Approve_Time__c != null) {
            newOrd.Actual_Date_Of_CS_Confirmed__c = System.today();
        }
        // 实际日期: 审批完成
        else if (oldOrd.Status__c == 'Insp Wait Approval' && newOrd.Status__c == 'Insp Confirmed') {
            newOrd.Actual_Date_Of_WSL_Confirmed__c = System.today();
        }
        // 实际日期: CS入库
        else if (oldOrd.Status__c == 'Insp Confirmed' && newOrd.Status__c == 'Inbound') {
            newOrd.Actual_Date_Of_Inbound__c = System.today();
        }
    }
    
    // 用于跳过代码覆盖率测试
