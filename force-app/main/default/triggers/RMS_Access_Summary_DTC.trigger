/** 创建DTCSummary时，设置Summary.Owner为Queue"RMS CFS/Digital Logistic" */
trigger RMS_Access_Summary_DTC on RTV_Summary__c (before insert) {
    // 新增DTC的Summary时
    for (RTV_Summary__c newSum: Trigger.new) {
        if (newSum.Summary_Type__c == 'DTC Takeback') {
            // 设置summary的Owner
            newSum.OwnerId = 
                newSum.DTC_Type__c == 'CFS'? (Id)newSum.Default_CFS_OwnerId__c: 
                newSum.DTC_Type__c == 'Digital'? (Id)newSum.Default_Digital_OwnerId__c:
                null;
        }
    }
