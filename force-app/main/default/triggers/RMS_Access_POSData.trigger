/**
 * 创建POSData时，修改POSData的Owner为对应ShipTo的Owner
 */
trigger RMS_Access_POSData on RTV_POS_Data__c (before insert) {
    Map<String, List<RTV_POS_Data__c>> posMapByName = new Map<String, List<RTV_POS_Data__c>>();
    
    // 遍历新建的PosData
    for (RTV_POS_Data__c newPos: Trigger.new) {
        if (newPos.Sales_Channel__c == '4691') {
            newPos.Sales_Channel__c = 'DIG';
        } 
        else {
            List<RTV_POS_Data__c> newPosLst = posMapByName.get(newPos.Name);
            newPosLst = newPosLst == null? new List<RTV_POS_Data__c>(): newPosLst;
            newPosLst.add(newPos);
            posMapByName.put(newPos.Name, newPosLst);
        }
    }
    
    // 跟据PosData.Name检索ShipTo
    for (RMS_Ship_To__c shipTo: [
        SELECT Id, OwnerId, Name, DTC_Code__c, Sales_Channel__c
        FROM RMS_Ship_To__c
        WHERE Name IN :posMapByName.KeySet() OR DTC_Code__c IN :posMapByName.KeySet()
    ]) {
        // 获取对应的当前更新中的PosData
        List<RTV_POS_Data__c> newPosLst = posMapByName.get(shipTo.Name);
        if (newPosLst == null) {
            newPosLst = posMapByName.get(shipTo.DTC_Code__c);
        }
        // 遍历该name的PosData，修改ownerId
        for (RTV_POS_Data__c newPos: newPosLst) {
            newPos.OwnerId = shipTo.OwnerId;
            newPos.Sales_Channel__c = shipTo.Sales_Channel__c;
        }
    }
    
