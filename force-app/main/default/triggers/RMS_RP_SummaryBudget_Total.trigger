trigger RMS_RP_SummaryBudget_Total on RTV_RP_Summary_Budget__c (after insert, after update, after delete) {
    // 处理中记录
    List<RTV_RP_Summary_Budget__c> tiggerRecords = Trigger.isDelete? trigger.old: trigger.new;
    
    // 待更新的programs
    Map<Id, RTV_Program__c> updPrograms = new Map<Id, RTV_Program__c>();
    
    // 遍历summaryBudget
    for (RTV_RP_Summary_Budget__c sumBdg: tiggerRecords) {
        RTV_RP_Summary_Budget__c oldsumBdg = Trigger.isUpdate? trigger.oldMap.get(sumBdg.Id): null;
        
        // 新增或删除时
        if (Trigger.isInsert || Trigger.isDelete
        // 或者更新'$NET'时
        || (Trigger.isUpdate && sumBdg.Tack_Back_Net__c != oldsumBdg.Tack_Back_Net__c)
        || (Trigger.isUpdate && sumBdg.QTY__c != oldsumBdg.QTY__c)
        ) {
            // 待更新program
            if (sumBdg.Return_Program__c != null) {
                RTV_Program__c program = new RTV_Program__c();
                program.Id = sumBdg.Return_Program__c;
                program.Budget_Amount_NET__c = 0;
                updPrograms.put(program.Id, program);
            }
        }
    }
    
    // 重新统计programs
    if (!updPrograms.isEmpty()) {
         for (AggregateResult grp: [
            SELECT Return_Program__c,
                SUM(QTY__c) sumQty,
                SUM(Tack_Back_Net__c) sumNet
            FROM RTV_RP_Summary_Budget__c
            WHERE Return_Program__c IN :updPrograms.KeySet()
            GROUP BY Return_Program__c
        ]) {
            // 更新program
            RTV_Program__c program = updPrograms.get((Id)grp.get('Return_Program__c'));
            program.Budget_QTY__c = (Decimal)grp.get('sumQty');
            program.Budget_Amount_NET__c = (Decimal)grp.get('sumNet');
        }
        // 更新（有addError()时会自动不执行）
        update updPrograms.values();
    }
    
    
    
