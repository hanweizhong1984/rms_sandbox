/** 创建DTC DEF Summary时，设置Summary.Owner为Queue"RMS CFS/Digital Logistic" */
trigger RMS_Access_DEF_Summary_DTC on RTV_DEF_Summary__c (after insert, after update) {
    List<RTV_DEF_Summary__c> updSums = new List<RTV_DEF_Summary__c>();
    
    // 新增DTC的Summary时
    for (RTV_DEF_Summary__c newDefSum: Trigger.new) { 
        RTV_DEF_Summary__c oldDefSum = Trigger.isUpdate? trigger.oldMap.get(newDefSum.Id): null;
        
        // DTC时
        if (newDefSum.DTC_Type__c != null) {
            // not start -> opening 时
            if (Trigger.isInsert 
            || (Trigger.isUpdate && oldDefSum.Active_Status__c == 'Not Start' && newDefSum.Active_Status__c == 'Opening')) {
                // 设置defsummary的owner
                if (newDefSum.DTC_Type__c == 'CFS' && newDefSum.Default_CFS_OwnerId__c != null) {
                    RTV_DEF_Summary__c updSum = new RTV_DEF_Summary__c();
                    updSum.Id = newDefSum.Id;
                    updSum.OwnerId = (Id)newDefSum.Default_CFS_OwnerId__c;
                    updSums.add(updSum);
                }
                else if (newDefSum.DTC_Type__c == 'DIG' && newDefSum.Default_Digital_OwnerId__c != null) {
                    RTV_DEF_Summary__c updSum = new RTV_DEF_Summary__c();
                    updSum.Id = newDefSum.Id;
                    updSum.OwnerId = (Id)newDefSum.Default_Digital_OwnerId__c;
                    updSums.add(updSum);
                }
            }
        }
    }
    if (updSums.size() > 0) {
        update updSums;
    }
    // ※另外，使用SharingRule设置共享Summary给logistics的下属
    
