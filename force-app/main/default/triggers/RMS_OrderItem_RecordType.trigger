/** 创建Item时，设置item的RecordType */
trigger RMS_OrderItem_RecordType on RTV_Order_Item__c (before insert) {
    Map<String, RecordType> itemTypes = RMS_CommonUtil.getRecordTypes('RTV_Order_Item__c');
    
    for (RTV_Order_Item__c newItem: Trigger.new) {
        // Baozun Seeding
        if (newItem.RTV_BaoZun_Seeding__c != null) {
            newItem.RecordTypeId = itemTypes.get('Baozun Seeding').Id;
        }
        // DTC Takeback
        else if (newItem.IsDTC__c == true && newItem.RTV_DEF_Summary__c == null) {
            newItem.RecordTypeId = itemTypes.get('DTC').Id;
        }
        // DTC Defective
        else if (newItem.IsDTC__c == true && newItem.RTV_DEF_Summary__c != null) {
            newItem.RecordTypeId = itemTypes.get('DTC Defective').Id;
        }
        // ***** ※ 为了不影响Takeback现有功能，以下的RecordType逻辑先不加 *****
        // // WSL Takeback
