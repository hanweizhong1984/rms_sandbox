public class RTV_Bth_SB_Del implements Database.Batchable<sObject> {
  final String query;
  public RTV_Bth_SB_Del(String query) {
    this.query = query;
  }

  public Database.QueryLocator start(Database.BatchableContext info) {
    return Database.getQueryLocator(this.query);
  }
  public void execute(Database.BatchableContext info, List<SObject> records) {
    delete records;
  }
  // The batch job finishes
  public void finish(Database.BatchableContext bc) {
    RTV_Bth_Summary_Budget bth = new RTV_Bth_Summary_Budget();
    Database.executebatch(bth, 1000);
  }
