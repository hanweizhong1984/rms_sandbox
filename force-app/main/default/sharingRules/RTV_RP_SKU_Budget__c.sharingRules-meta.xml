<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>GlobalSkuBudget</fullName>
        <accessLevel>Read</accessLevel>
        <description>SkuBudget没有指定AccountGroup以下维度时，分享给所有用户</description>
        <label>GlobalSkuBudget</label>
        <sharedTo>
            <roleAndSubordinates>RMS_Admin</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>Is_Global_Sku_Budget__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>ToConverse</fullName>
        <accessLevel>Edit</accessLevel>
        <description>所有Converse内部觉色都有读写权限</description>
        <label>ToConverse</label>
        <sharedTo>
            <roleAndSubordinates>RMS_Converse</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>notEqual</operation>
            <value/>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
</SharingRules>
