<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>SharToConverse</fullName>
        <accessLevel>Edit</accessLevel>
        <description>所有Converse内部觉色都有读写权限（如果是Profile中允许的话）</description>
        <label>SharToConverse</label>
        <sharedTo>
            <roleAndSubordinates>RMS_Converse</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>Status__c</field>
            <operation>notEqual</operation>
            <value/>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>ShareToLF</fullName>
        <accessLevel>Edit</accessLevel>
        <description>所有LF人员都可编辑（PostToLF时）（如果Profile允许的话）</description>
        <label>ShareToLF</label>
        <sharedTo>
            <roleAndSubordinates>RMS_IDC_LF</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>Status__c</field>
            <operation>notEqual</operation>
            <value/>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>DTC_ShareTo_CFS_Sub</fullName>
        <accessLevel>Read</accessLevel>
        <label>DTC_ShareTo_CFS_Sub</label>
        <sharedTo>
            <roleAndSubordinates>RMS_CFS_Logistic_Confirm</roleAndSubordinates>
        </sharedTo>
        <sharedFrom>
            <role>RMS_CFS_Logistic_Confirm</role>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>DTC_ShareTo_Digital_Sub</fullName>
        <accessLevel>Read</accessLevel>
        <label>DTC_ShareTo_Digital_Sub</label>
        <sharedTo>
            <roleAndSubordinates>RMS_DIG_Logistic_Confirm</roleAndSubordinates>
        </sharedTo>
        <sharedFrom>
            <role>RMS_DIG_Logistic_Confirm</role>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
