    <rules>
        <fullName>RTV DEF Order PTLF</fullName>
        <actions>
            <name>RTV_DEF_Order_PTLF</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>DEF Order Post to LF,发送邮件给LF</description>
        <formula>RTV_DEF_Summary__c != null &amp;&amp; ISPICKVAL(Status__c, &apos;POST to LF&apos;) &amp;&amp; ISBLANK(TEXT(RTV_DEF_Summary__r.DTC_Type__c))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>RTV DTC DEF Order Completed%28To CS%29</fullName>
        <actions>
            <name>RTV_DTC_DEF_Order_Completed_To_CS</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>DTC》DEF》Order在Completed时：发送邮件给CS</description>
        <formula>IsDTC__c == true &amp;&amp; RTV_DEF_Summary__c != null &amp;&amp; ISPICKVAL(Status__c, &apos;Completed&apos;)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
