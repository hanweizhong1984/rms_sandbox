<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>New_Status_Pending</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW() , Text(Program_Status__c ) != &apos;Pending&apos;)</errorConditionFormula>
    <errorDisplayField>Program_Status__c</errorDisplayField>
    <errorMessage>New program status is pending by default</errorMessage>
</ValidationRule>
