<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CanNotChangeDateAfterInsert</fullName>
    <active>true</active>
    <errorConditionFormula>OR(ISCHANGED( Start_Date__c ) ,ISCHANGED( End_Date__c ) )</errorConditionFormula>
    <errorMessage>Can Not Change Date Information After Insert!</errorMessage>
</ValidationRule>
